var _gaq = _gaq || [];

!function(document,nbrut) {
    'use strict';

	_gaq.push(['_setAccount', '@config.tracking.analytics']);
	_gaq.push(['_trackPageview']);

    var protocol = document.location.protocol === 'https:' ? 'https://ssl' : 'http://www';

    nbrut.analytics.fetch(protocol + '.google-analytics.com/ga.js');
    nbrut.analytics.onHistoryChange(function(data){
		_gaq.push(['_trackPageview', data.url]);
    });
}(document,nbrut);